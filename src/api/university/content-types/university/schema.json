{
  "kind": "collectionType",
  "collectionName": "universities",
  "info": {
    "singularName": "university",
    "pluralName": "universities",
    "displayName": "University",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "university_name": {
      "type": "string",
      "unique": true
    },
    "university_link": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "opendate": {
      "type": "date",
      "required": true
    },
    "closingdate": {
      "type": "date",
      "required": true
    },
    "faculties": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::faculty.faculty",
      "mappedBy": "university"
    },
    "image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
